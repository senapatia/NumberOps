    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>AddTheNumber</servlet-name>
    <servlet-class>operation.number.app.AddNumber</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddTheNumber</servlet-name>
    <url-pattern>/Add</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SquareTheNumber</servlet-name>
    <servlet-class>operation.number.app.SquareNumber</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SquareTheNumber</servlet-name>
    <url-pattern>/Square</url-pattern>
  </servlet-mapping>
</web-app>